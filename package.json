{
  "name": "codemirror-lang-liquid",
  "version": "0.1.1",
  "description": "Liquid language support for CodeMirror",
  "scripts": {
    "build": "rollup -c",
    "build:demo": "cd demo/ && npm run build",
    "start:demo": "cd demo/ && npm start",
    "test": "mocha test/test.js"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "dependencies": {
    "@codemirror/autocomplete": "^6.9.1",
    "@codemirror/lang-html": "github:codemirror/lang-html",
    "@codemirror/language": "^6.0.0",
    "@lezer/highlight": "^1.0.0",
    "@lezer/lr": "^1.0.0"
  },
  "devDependencies": {
    "@lezer/generator": "^1.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "codemirror": "^6.0.1",
    "http-server": "^14.1.1",
    "mocha": "^9.0.1",
    "rollup": "^2.60.2",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.3.4"
  },
  "license": "MIT"
}
